@page "{restaurantId:int}"


@model ASP.NET_Restaurang.Pages.Restaurants.DetailModel
@{
    ViewData["Title"] = "Detail";
}
@if (Model.Message != null)
{
    <div class="alert alert-info text-center">
        <p>@Model.Message</p>
    </div>
}


<h2>@Model.Restaurant.Name</h2>
<div>Id: @Model.Restaurant.Id</div>
<div>
    Location: @Model.Restaurant.Location
</div>
<div>
    Cuisine: @Model.Restaurant.Cuisine
</div>



<a asp-page="./List" class="btn btn-info">Alla varor</a>

